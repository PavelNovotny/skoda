              },
              "ngram": {
                "type": "text",
                "analyzer": "index_analyzer_ngram",
                "search_analyzer": "search_analyzer_term",
                "term_vector": "with_positions_offsets"
              }
            }
          },
          "dealer_street": {
            "type": "text",
            "fields": {
              "raw": {
                "type": "keyword",
                "normalizer": "keyword_normalizer"
              },
              "ngram": {
                "type": "text",
                "analyzer": "index_analyzer_ngram",
                "search_analyzer": "search_analyzer_term",
